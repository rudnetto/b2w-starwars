[ignore]
.*/flow-typed/*.
.*/.vscode/*.
.*/build/*.
.*/coverage/*.
**/node_modules/*

[include]
./src/

[libs]

[lints]

[options]
module.system.node.resolve_dirname=node_modules
suppress_comment= \\(.\\|\n\\)*\\flow-disable-next-line
module.system.node.resolve_dirname=src
module.file_ext=.js
module.file_ext=.json
module.file_ext=.jsx
module.file_ext=.css
module.file_ext=.scss

[strict]
